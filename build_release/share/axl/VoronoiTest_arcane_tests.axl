<!-- ###################################################################### -->

<!-- Options du jeu de données pour le service de test du maillage -->

<service name="VoronoiTest" version="1.0" type="caseoption" parent-name="Arcane::BasicUnitTest" namespace-name="ArcaneTest">
 <interface name="Arcane::IUnitTest" inherited="false" />

	<variables>
    <variable
      field-name="cell_flags"
      name="CellFlags"
      data-type="real"
      item-kind="cell"
      dim="0"
      dump="false"
      need-sync="true" 
    />
    <variable
      field-name="domain_id"
      name="DomainId"
      data-type="integer"
      item-kind="cell"
      dim="0"
      dump="false"
      need-sync="true" 
    />
    <variable
      field-name="face_flags"
      name="FaceFlags"
      data-type="real"
      item-kind="face"
      dim="0"
      dump="false"
      need-sync="true" 
    />
  </variables>

 <options/>
</service>
