<?xml version="1.0" encoding="ISO-8859-1" ?>
<service name="AlephTestSchemeFaces" version="1.0" type="caseoption">
  <interface name="ArcaneTest::AlephTestScheme" />
  <variables>
    <variable field-name="cell_temperature" name="CellTemperature" data-type="real" item-kind="cell" dim="0" dump="true" need-sync="true" />
    <variable field-name="face_temperature" name="FaceTemperature" data-type="real" item-kind="face" dim="0" dump="false" need-sync="true" />
    <variable field-name="cell_matrix_idx" name="CellMatrixIdx" data-type="integer" item-kind="cell" dim="0" dump="false" need-sync="false" />
    <variable field-name="cell_coefs" name="CellCoefs" data-type="real" item-kind="cell" dim="0" dump="false" need-sync="false" />
    <!--variable field-name="row_nb_elems" name="RowNbElems" data-type="integer" item-kind="cell" dim="0" dump="false" need-sync="false" /-->
  </variables> 
  <options>
    <simple name="amr" type="bool" default="false"/>
  </options>
</service>
