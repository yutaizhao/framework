<?xml version="1.0" ?>
<arcane-config code-name="HelloWorld">
  <time-loops>
    <time-loop name="HelloWorldLoop">
      <title>HelloWorld</title>
      <description>Boucle en temps de l'exemple Arcane Hello World</description>

      <modules>
        <module name="HelloWorld" need="required" />
      </modules>

      <entry-points where="compute-loop">
        <entry-point name="HelloWorld.PrintHelloWorld" />
      </entry-points>
    </time-loop>
  </time-loops>
</arcane-config>
