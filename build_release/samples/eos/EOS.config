<?xml version="1.0" ?>
<arcane-config code-name="EOS">
  <time-loops>
    <time-loop name="EOSLoop">
      <title>EOS</title>
      <description>Boucle en temps du module d'exemple de calcul des EOS</description>

      <modules>
        <module name="EOS" need="required" />
      </modules>

      <entry-points where="init">
        <entry-point name="EOS.InitEOS" />
      </entry-points>
      <entry-points where="compute-loop">
        <entry-point name="EOS.ComputeEOS" />
      </entry-points>
    </time-loop>
  </time-loops>
</arcane-config>
