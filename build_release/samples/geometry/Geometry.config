<?xml version="1.0"?>
<arcane-config code-name="Geometry">
  <time-loops>
    <time-loop name="GeometryLoop">
      <title>Geometry</title>
      <description>Exemple de calcul géométrique</description>

      <modules>
	<module name="Geometry" need="required" />
      </modules>

      <entry-points where="init">
	<entry-point name="Geometry.Init" />
      </entry-points>

      <entry-points where="compute-loop">
	<entry-point name="Geometry.ComputeSurfacesAndVolumes" />
      </entry-points>
    </time-loop>
  </time-loops>
</arcane-config>
